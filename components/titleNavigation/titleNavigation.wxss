.navBar .return image {
  width: 18rpx;
  height: 32rpx;
  /* margin-top: 16rpx; */
  vertical-align: middle;
  margin-right: 10rpx;
}

.return text{
  vertical-align: middle;
  font-size: 26rpx;
  line-height: 26rpx;
}

.navBar .home {
  display: inline-block;
  /* width: 50%; *//* text-align: center; */
  margin-left: 22rpx;
  vertical-align: middle;
}

.navBar .home image {
  width: 34rpx;
  height: 33rpx;
  margin-top: 16rpx;
}

/* 详情页结束 */

/* 分类开始 */

.navBar01 {
  position: absolute;
  left: 27rpx;
  top: 50%;
  transform: translateY(-50%);
  z-index: 6;
}

.navBar01 text {
  font-size: 38rpx;
  color: #000;
  vertical-align: middle;
  display: inline-block;
  margin-right: 30rpx;
}

.navBar01 image {
  width: 322rpx;
  height: 56rpx;
  vertical-align: middle;
  display: inline-block;
}

/* 分类结束 */


.up{
  display: inline-block;
  width:0;
  height:0;
  border-right:10rpx solid transparent;
  border-left:10rpx solid transparent;
  border-top:10rpx solid #333;
  margin-left: 10rpx;
}
.down{
  display: inline-block;
  width:0;
  height:0;
  border-right:10rpx solid transparent;
  border-left:10rpx solid transparent;
  border-bottom:10rpx solid #333;
  margin-left: 10rpx;
}
.titleMoreP{
  position: fixed;
    top: 89rpx;
    left: 0;
