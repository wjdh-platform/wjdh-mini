.navigate-container {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 9999;
  background: #fff !important;
  /* text-align: center; */
}

.navigate-box {
  position: relative;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
}

.navigate-text {
  font-size: 32rpx;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
}

.navigate-text.androidLeft {
  position: absolute;
  left: 75rpx;
  top: 50%;
  transform: translateY(-50%);
}

.navigate-iconBox {
  display: block;
  width: 45rpx;
  height: 45rpx;
  position: absolute;
  left: 15rpx;
  top: 50%;
  transform: translateY(-50%);
}

.navigate-icon {
  display: block;
  width: 18rpx;
  height: 32rpx;
  position: absolute;
  left: 15rpx;
  top: 55%;
  transform: translateY(-50%);
}

/* 搜索结束 */

/* 详情页开始 */

.navBar {
  position: absolute;
  left: 20rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 174rpx;
  height: 64rpx;
  /* background: rgba(255, 255, 255, 0.4); */
  border-radius: 40rpx;
  z-index: 8;
}

.navBar .return {
  width: 30%;
  position: relative;
  text-align: center;
  display: inline-block;
  vertical-align: middle;
  
  margin-top: 10rpx;
}
.title{
  font-size: 28rpx;
}
.title text{
  font-size: 27rpx;
  color: #333;
}

