/* components/uploadImg.wxss */
.msgColumnO{
  padding: 30rpx 40rpx;
}
.uploadPhoto{
  margin-top: 30rpx;
}
.photoClo{
  display: inline-block;
}
.photograph{
  position: absolute;
  right: 40rpx;
  top: 50%;
  transform: translateY(-50%);
  z-index: 3;
  font-size: 22rpx;
  color: #424242;
}
.photograph image{
  width: 39rpx;
  height: 36rpx;
  display: block;
  margin-left: 25rpx;
}
.photoImg{
  width: 144rpx;
  height: 144rpx;
  border-radius: 10rpx;
}
.photoClo{
  position: relative;
}
.imgClose{
  position: absolute;
  right: -15rpx;
  top: -15rpx;
  width: 35rpx;
  height: 35rpx;
  z-index: 3;
}
.addImg{
  width: 144rpx;
  height: 144rpx;
  display: inline-block;
  border-radius: 10rpx;
  margin-left: 20rpx;
  background: #F8F8F8 url("data:image/png;base64,") no-repeat center center;
  background-size: 46rpx 46rpx;

}