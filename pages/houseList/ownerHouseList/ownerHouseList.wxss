/* pages/houseList/ownerHouseList/ownerHouseList.wxss */
page{
  /* background: #e8e8e8; */
}
.listColumn{
  width: 690rpx;
  height: 186rpx;
  background: #FFFFFF;
  box-shadow: 0px 0px 12rpx 1rpx rgba(8, 8, 8, 0.07);
  border-radius: 17rpx;
  margin: 0 auto;
  display: flex;
  align-items: center;
  /* flex-direction: row; */
  justify-content: space-between;
  padding: 0 40rpx;
  box-sizing: border-box;
  margin-top: 30rpx;
}
.changeIntro,.Unbundling{
  display: inline-block;
  width: 107rpx;
  height: 40rpx;
  background: #3ECBFF;
  opacity: 0.8;
  border-radius: 20rpx;
  text-align: center;
  font-size: 23rpx;
  color: #fff;
  line-height: 40rpx;
}
.Unbundling{
  background: #FF3E3E;
  margin-left: 40rpx;
}
.houseName{
  color: #333;
  font-size: 32rpx;
  line-height: 32rpx;
  margin-bottom: 30rpx;
}
.houseType{
  color: #50DABE;
  font-size: 30rpx;
}
.fail{
  color: red;
}
.listNone{
  text-align: center;
  font-size: 28rpx;
  color: #666; 
  margin-top: 100rpx;
}
.introduction text{
  color: red;
}
.introduction{
  color: #666;
  font-size: 26rpx;
}
