<view class="propertyMain">
  <view class="clouds">
    <!-- <image class="cloud" src="https://tc.mg.cool/mini/images/indexBg/cloud1.png"></image>
   <image class="cloud1" src="https://tc.mg.cool/mini/images/indexBg/cloud2.png"></image>
