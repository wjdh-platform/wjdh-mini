/* pages/property/property.wxss */
page{
  /* padding-top: 130rpx; */
  /* padding-bottom: 100rpx; */
}
.banner{
  width: 690rpx;
  margin: 0 auto;
  height: 350rpx;
  display: block;
}
.tabCol{
  width: 25%;
  display: inline-block;
  text-align: center;
  margin: 16rpx 0 0;
}
.closePopup{
  background: url("data:image/png;base64,") no-repeat;
  background-size: cover;
  position: absolute;
  right: 30rpx;
  top: 30rpx;
  z-index: 10;
  width: 20rpx;
  height: 20rpx;
}
.tabCol image{
  /* width: 155rpx;
  height: 155rpx; */
  width: 140rpx;
  height: 140rpx;
}
.bindText{
  font-size: 22rpx;
  color: #333;
  
  line-height: 22rpx;
}
.activationP{
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, .4);
  z-index: 99;
}
.activitionCont{
  width: 522rpx;
  /* height: 461rpx; */
  padding: 40rpx 30rpx 60rpx;
  background: #fff;
  border-radius: 15rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  box-sizing: border-box;
}
.popupTitle{
  text-align: center;
  color: #333333;
  font-size: 30rpx;
}
.photoText{
  border-bottom:  solid 1rpx #E9E9E9;
  padding: 60rpx 0 10rpx;
  position: relative;
}
.photoIcon{
  width: 22rpx;
  height: 30rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 22rpx;
}
.userIcon{
  width: 26rpx;
  height: 26rpx;
}
.codeIcon{
  width: 24rpx;
  height: 26rpx;
}
.phototextP{
  font-size: 28rpx;
  color: #282828;
  display: inline-block;
  vertical-align: middle;
}
.phototextPP{
  color: #B1B1B1;
}
.codeBtn{
  position: absolute;
  right: 0;
  top: 60rpx;
  z-index: 4;
  font-size: 28rpx;
  color: #50DABE;
}
.verificationBtn{
  width: 100%;
  height: 70rpx;
  background: #50DABE;
  border-radius: 35rpx;
  margin: 60rpx auto 0;
  text-align: center;
  line-height: 70rpx;
  font-size: 28rpx;
  color: #fff;
}
.column01{
  display: flex;
  align-items: center;
  border-bottom: solid 1rpx #ececec;
  height: 94rpx;
  margin-top: 20rpx;
}
.column01 .title{
  font-size: 28rpx;
  color: #424242;
  width: 180rpx;
}
.column01 picker{
  font-size: 28rpx;
}
.arrow{
  width: 14rpx;
  height: 24rpx;
}
picker{
  flex: 1;
  text-align: left;
  font-size: 30rpx;
  color: #424242;
}
.tipsText{
  font-size: 27rpx;
  color: #555;
  margin: 30rpx 0;
}
.tipsText text{
  display: block;
  line-height: 40rpx;
}
.tipsBtn{
  /* float: right; */
  text-align: center;
  font-size: 28rpx;
  color: #50DABE;
}
.newsLeft{
  width: 640rpx;
}
.newsListTitle{
  border-bottom: solid 1rpx #e9e9e9;
  font-weight: bold;
  padding: 50rpx 0 20rpx;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #333;
}
.newsListTitle image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.newsCol{
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: solid 1rpx #e9e9e9;
  padding: 30rpx 0;
}
.newsCol:last-child{
  border: none;
}
.newsImg{
  width: 320rpx;
  height: 150rpx;
}
.newsList{
  padding: 0 30rpx;
}
.newsTitle{
  font-size: 30rpx;
  color: #333;
  line-height: 45rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.timeRes{
  font-size: 24rpx;
  color: #989898;
  margin-top: 20rpx;
}
.timeRes text{
  margin: 0 30rpx 0 0;
}
.hoverTab{
  opacity: 0.5;
  border-radius: 50%;
  transform: scale(1);
  transition: all 0.01s ease-in-out;
 }
