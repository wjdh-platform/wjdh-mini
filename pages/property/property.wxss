}
.column01 .title{
  font-size: 28rpx;
  color: #424242;
  width: 180rpx;
}
.column01 picker{
  font-size: 28rpx;
}
.arrow{
  width: 14rpx;
  height: 24rpx;
}
picker{
  flex: 1;
  text-align: left;
  font-size: 30rpx;
  color: #424242;
}
.tipsText{
  font-size: 27rpx;
  color: #555;
  margin: 30rpx 0;
}
.tipsText text{
  display: block;
  line-height: 40rpx;
}
.tipsBtn{
  /* float: right; */
  text-align: center;
  font-size: 28rpx;
  color: #50DABE;
}
.newsLeft{
  width: 640rpx;
}
.newsListTitle{
  border-bottom: solid 1rpx #e9e9e9;
  font-weight: bold;
  padding: 50rpx 0 20rpx;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #333;
}
.newsListTitle image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.newsCol{
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: solid 1rpx #e9e9e9;
  padding: 30rpx 0;
}
.newsCol:last-child{
  border: none;
}
.newsImg{
  width: 320rpx;
  height: 150rpx;
}
.newsList{
  padding: 0 30rpx;
}
.newsTitle{
  font-size: 30rpx;
  color: #333;
  line-height: 45rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.timeRes{
  font-size: 24rpx;
  color: #989898;
  margin-top: 20rpx;
}
.timeRes text{
  margin: 0 30rpx 0 0;
}
.hoverTab{
  opacity: 0.5;
  border-radius: 50%;
  transform: scale(1);
  transition: all 0.01s ease-in-out;
 }
